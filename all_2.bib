@article{krauseSpatiotemporalEvolutionAustralasian2019a,
  title = {Spatio-Temporal Evolution of {{Australasian}} Monsoon Hydroclimate over the Last 40,000 Years},
  author = {Krause, Claire E. and Gagan, Michael K. and Dunbar, Gavin B. and Hantoro, Wahyoe S. and Hellstrom, John C. and Cheng, Hai and Edwards, R. Lawrence and Suwargadi, Bambang W. and Abram, Nerilie J. and Rifai, Hamdi},
  date = {2019-05-01},

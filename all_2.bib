@article{proskeHoloceneDiatomRecords2017a,
  title = {Holocene Diatom Records of Wetland Development near {{Weipa}}, {{Cape York}}, {{Australia}}},
  author = {Proske, Ulrike and Stevenson, Janelle and Seddon, Alistair W. R. and Taffs, Kathryn},
  date = {2017-06-08},
  journaltitle = {Quaternary International},
  shortjournal = {Quaternary International},
  volume = {440},

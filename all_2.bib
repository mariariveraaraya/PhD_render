@article{wangGlobalMonsoonTime2017,
  title = {The Global Monsoon across Time Scales: {{Mechanisms}} and Outstanding Issues},
  shorttitle = {The Global Monsoon across Time Scales},
  author = {Wang, Pin Xian and Wang, Bin and Cheng, Hai and Fasullo, John and Guo, ZhengTang and Kiefer, Thorsten and Liu, ZhengYu},
  date = {2017-11-01},
  journaltitle = {Earth-Science Reviews},
  shortjournal = {Earth-Science Reviews},
  volume = {174},
  pages = {84--121},
  issn = {0012-8252},
  doi = {10.1016/j.earscirev.2017.07.006},
  url = {http://www.sciencedirect.com/science/article/pii/S0012825216302070},
  urldate = {2018-07-20},
  abstract = {The present paper addresses driving mechanisms of global monsoon (GM) variability and outstanding issues in GM science. This is the second synthesis of the PAGES GM Working Group following the first synthesis “The Global Monsoon across Time Scales: coherent variability of regional monsoons” published in 2014 (Climate of the Past, 10, 2007–2052). Here we introduce the GM as a planetary scale circulation system and give a brief accounting of why it exhibits regional structure. The primary driver of the GM is solar insolation, and the specific features in the underlying surface, such as land-sea distribution, topography, and oceanic circulations, are mainly responsible for the differences among regional monsoon systems. We then analyze the monsoon formation mechanisms, together with the major processes that drive monsoon variations at various timescales, including external forcings and internal feedbacks. On long time scales, external forcings often induce variability on a global scale, whereas short-term variability in regional monsoon systems is usually caused by internal feedbacks within the climate system. Finally, a number of debatable issues are discussed, with an emphasis on time scales beyond the instrumental record. These include the dual nature of the monsoon as wind and rain, the meaning of oxygen isotope in hydrological cycle, in particular of speleothem δ18O, the role of ice-sheet in monsoon variations, etc. In general, the GM as a system comprises a hierarchy of regional and local monsoons with various degrees of similarity, though all show coherent variability driven by a common solar forcing. The goal of the GM concept, therefore, is by no means to replace or diminish research on the regional monsoons, but to help dissect the mechanisms and controlling factors of monsoon variability at various temporal-spatial scales.},
  file = {C\:\\Users\\Maria Jose Rivera\\Zotero\\storage\\ALIWEXGS\\Wang et al. - 2017 - The global monsoon across time scales Mechanisms .pdf;C\:\\Users\\Maria Jose Rivera\\Zotero\\storage\\DHFNQSJ7\\S0012825216302070.html},
  keywords = {Climate variability,Hydrological cycle,Monsoon,Monsoon mechanism,Precipitation,Solar insolation}
}

@article{wangRadiocarbonDatingSoil1996,
  title = {Radiocarbon {{Dating}} of {{Soil Organic Matter}}},
  author = {Wang, Yang and Amundson, Ronald and Trumbore, Susan},
  date = {1996-05-01},
  journaltitle = {Quaternary Research},
  shortjournal = {Quaternary Research},
  volume = {45},
  pages = {282--288},
